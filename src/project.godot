config_version=5

[application]

config/name="Anubis"
config/version="v0.1.0"
run/main_scene="res://Scenes/MainScene.tscn"
config/features=PackedStringArray("4.3", "C#")
config/icon="res://Icon.svg"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Anubis"

[editor]

naming/default_signal_callback_name="On{node_name}{signal_name}"
naming/default_signal_callback_to_self_name="On{signal_name}"
naming/scene_name_casing=1
naming/script_name_casing=1

[file_customization]

folder_colors={
"res://Resources/": "yellow",
"res://Scenes/": "green",
"res://Scripts/": "red",
"res://Sprites/": "purple"
}

[gui]

theme/custom="res://Theme.tres"
theme/custom_font="res://Monogram.ttf"
theme/default_font_generate_mipmaps=true

